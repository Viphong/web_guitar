
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/Shared_layout.cshtml";
    var listnewVideo = (List<Models.Framework.Video>)ViewBag.NewVideo;
}
@Html.Partial("~/Views/Home/MainSliderPartial.cshtml")
 <!-- Primary left -->
<div id="primary-left">
    @foreach (var item in listnewVideo)
    {
          
        var url = "/video/" + @item.MetaTitle + "/" + @item.Mavideo;
        <!-- Article standard -->
        <div class="article-standard">
            <div class="post-video">
                <div class="fluid-width-video-wrapper" style="padding-top: 85.7143%;">
                <iframe src="@item.Link" ></iframe>
                </div>
            </div>
            <div class="entry-content">
                <h1 class="entry-title"><a href="@url">@item.Tenvideo</a></h1>
                <div class="entry-meta">
                    <div class="review-stars">
                        <span class="star-full"></span>
                        <span class="star-full"></span>
                        <span class="star-full"></span>
                        <span class="star-full"></span>
                        <span class="star-full"></span>
                    </div>
                    <div class="description-em">
                        <span class="by-view-number">132</span>
                        <span class="by-date">1 day ago</span>
                        <span class="by-comments"><a href="#">42</a></span>
                    </div>
                </div>
                <p>Hooking up with Double Identity for another sic collab shoe. The last one already rockin these new ones out and about. Eget nunc nunc.</p>
                <a href="@url" class="read-more">Read more</a>
                <span class="format-post"><i class="icon-play-sign"></i></span>
            </div>
        </div>
    }
    
    <!-- Clear floating div -->
    <div class="clear"></div>
</div>
